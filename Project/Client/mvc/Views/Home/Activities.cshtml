@model List<mvc.Models.ActivityData>

@{
    ViewBag.Title = "Activities";
    Layout = "_Layout";
}

<h2>Weather</h2>


<table>
    @foreach (var item in Model)
     {
        <tr>
            <td>@item.Name</td>
            <td>@item.Duration</td>
            <td>@item.Year</td>
            <td>@item.Level</td>
        </tr>
     }
</table>